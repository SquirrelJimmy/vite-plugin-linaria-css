{
  "name": "vite-plugin-linaria-css",
  "version": "1.0.0",
  "author": "SexySquirrel",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:lib": "NODE_ENV=legacy babel src --out-dir dist/lib --extensions '.js,.jsx,.ts,.tsx' --source-maps --delete-dir-on-start",
    "build:esm": "babel src --out-dir dist/esm --extensions '.js,.jsx,.ts,.tsx' --source-maps --delete-dir-on-start",
    "build": "yarn build:lib && yarn build:esm",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@linaria/babel-preset": "^3.0.0-beta.3",
    "@rollup/pluginutils": "^4.1.0",
    "find-yarn-workspace-root": "^2.0.0",
    "mkdirp": "^1.0.4",
    "normalize-path": "^3.0.0",
    "vite": "^2.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.12",
    "@types/babel__traverse": "^7.11.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.31",
    "@types/normalize-path": "^3.0.0"
  }
}
